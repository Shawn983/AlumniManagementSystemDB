{
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "/static/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ]
}
