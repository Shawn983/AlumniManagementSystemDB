{
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/static/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ]
}
